{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": false,
        "class-name": false,
        "curly": [true, "ignore-same-line"],
        "interface-name": false,
        "max-classes-per-file": false,
        "no-empty": false,
        "no-namespace": false,
        "no-shadowed-variable": false,
        "prefer-for-of": false,
        "variable-name": [ true, "allow-leading-underscore" ]
    },
    "rulesDirectory": []
}